    <h1><%= @current_user.username %>'s Review of <%= @review.studio.name %> </h1>
    <h3><%= @review.rating %>/5</h3>
    <p><%= @review.content %></p>
    <br><br>

    <span><%= link_to "Edit Review Details", edit_review_path(@review) %></span>
    <span><%= link_to "Delete Review", @review, :method => :delete %></span>
    <span><%= link_to "Your Reviews", user_reviews_path(@current_user) %></span>
