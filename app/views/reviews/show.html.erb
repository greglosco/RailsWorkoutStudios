 <% if @review.persisted? %> 
    <h1><%= @current_user.username %>'s Review of <%= @review.studio.name %> </h1>
    <h3><%= @review.rating %>/5</h3>
    <p><%= @review.content %></p>

    <%= link_to "Edit Review Details", edit_review_path(@review) %><br><br>
    <%= link_to "Delete Review", @review, :method => :delete %><br><br>
    <%= link_to "Your Reviews", user_reviews_path(@current_user) %>
 <% end %>