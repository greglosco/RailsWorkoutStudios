<h1>All Studios</h1>

<ol>
<% @studios.each do |studio| %>
    <li><%= link_to studio.name, user_studio_path(@current_user, studio) %> - <%= studio.category %> - <%= link_to "Edit Studio Details", edit_studio_path(studio) %> - <%= link_to "Delete Studio", studio, :method => :delete %></li> - Added By:<%=  studio.user.username %><br><br>
<% end %>
</ol>

<%= link_to "Add a new studio", new_user_studio_path(@current_user) %><br>
<%= link_to "Back to Account Page", user_path(@current_user) %>